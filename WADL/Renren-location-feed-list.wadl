</param>
<param name="longitude" style="query" type="xsd:double">
</param>
<param name="latitude" style="query" type="xsd:double">
</param>
</request>
<response status="200"> 
<representation mediaType="application/json" />
</response> 
</method>
</resource>
</resources>
</application>