<method name="POST">
<request>
<param name="banner" style="query" type="xsd:base64Binary" required="true">
</param>
<param name="width" style="query" type="xsd:int">
</param>
<param name="height" style="query" type="xsd:int">
</param>
<param name="offset_left" style="query" type="xsd:int">
</param>
<param name="offset_top" style="query" type="xsd:int">
</param>
</request>
<response> 
<representation mediaType="application/json" />
</response> 
</method>
</resource>
</resources>
</application>